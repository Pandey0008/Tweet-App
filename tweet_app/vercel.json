{
    "builds": [
        {
            "src": "tweet_app/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "buildCommand":"bash setup.sh"
        }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "tweet_app/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}
